{
  "name": "websitetest",
  "type": "module",
  "version": "1.0.0",
  "description": "Enterprise-ready Astro static website template with advanced features",
  "keywords": [
    "astro",
    "tailwind",
    "react",
    "typescript",
    "enterprise",
    "static-site"
  ],
  "author": "Enterprise Template",
  "homepage": "https://tariqdude.github.io/WebsiteTest",
  "repository": {
    "type": "git",
    "url": "https://github.com/tariqdude/WebsiteTest.git"
  },
  "bugs": {
    "url": "https://github.com/tariqdude/WebsiteTest/issues"
  },
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro check && astro build",
    "build:prod": "NODE_ENV=production astro build",
    "preview": "astro preview --host",
    "preview:prod": "astro preview --host --port 4173",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "analyze": "astro build && npx @astro/cli info",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u && npm install",
    "lighthouse": "npx lighthouse http://localhost:4321 --output=html --output-path=./lighthouse-report.html",
    "bundle:analyze": "npx vite-bundle-analyzer dist",
    "prepare": "echo 'Setting up project...'",
    "postinstall": "echo 'Dependencies installed successfully!'"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "@headlessui/react": "^2.2.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "aos": "^2.3.4",
    "astro": "^5.12.8",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "gsap": "^3.13.0",
    "intersection-observer": "^0.12.2",
    "lucide-react": "^0.536.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/ts-plugin": "^1.10.4",
    "@types/aos": "^3.0.7",
    "@types/node": "^22.17.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sass": "^1.89.2",
    "vitest": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^25.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "happy-dom": "^16.8.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
