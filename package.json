{
  "name": "astro-showcase",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "astro dev",
    "dev:verbose": "VERBOSE_LOGS=true astro dev",
    "dev:host": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:production": "NODE_ENV=production astro build",
    "build:gh-pages": "NODE_ENV=production astro build --site https://tariqdude.github.io --base /WebsiteTest",
    "build:analyze": "npm run deploy:health && npm run build:gh-pages",
    "preview": "astro preview",
    "preview:host": "astro preview --host",
    "check": "astro check",
    "check:types": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "astro check && tsc --noEmit && npm run format:check",
    "clean": "rm -rf dist .astro node_modules/.vite",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "deploy:health": "node tools/deployment-manager.mjs health",
    "deploy:validate": "node tools/deployment-manager.mjs validate",
    "deploy:fix": "node tools/deployment-manager.mjs fix",
    "deploy:build-test": "node tools/deployment-manager.mjs build-test",
    "deploy:prep": "npm run deploy:fix && npm run build:gh-pages",
    "test:ssr": "node tools/ssr-test.mjs",
    "test:deploy": "npm run test:ssr && npm run deploy:validate",
    "diagnostics": "npm run deploy:health --verbose",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.1",
    "@astrojs/preact": "^4.1.0",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/solid-js": "^5.1.0",
    "@astrojs/svelte": "^7.1.0",
    "@astrojs/tailwind": "^5.1.2",
    "@astrojs/vue": "^5.1.0",
    "@hookform/resolvers": "^5.1.1",
    "@monaco-editor/react": "^4.7.0",
    "@types/three": "^0.178.1",
    "astro": "5.12.1",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "framer-motion": "^11.15.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.468.0",
    "monaco-editor": "^0.52.2",
    "preact": "^10.26.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.60.0",
    "react-hot-toast": "^2.5.2",
    "sharp": "^0.34.3",
    "solid-js": "^1.9.7",
    "svelte": "^5.36.13",
    "tailwindcss": "^3.4.15",
    "three": "^0.178.0",
    "vue": "^3.5.13",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@astrojs/partytown": "^2.1.4",
    "@types/d3": "^7.4.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro-seo": "^0.8.4",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "^5.8.3"
  }
}
